class Solution {
public:
    int countOdds(int low, int high) {
        int temp;
        temp=(high-low)/2;
        if(high%2!=0 && low%2!=0)
        {
            temp++;
        }
        else if(low%2!=0 )
        {
            temp++;
        }
        else if(high%2!=0)
        {
             temp++;
        }
        return temp;
    }
};
